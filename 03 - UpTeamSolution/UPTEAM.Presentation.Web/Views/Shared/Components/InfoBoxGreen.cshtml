@model UPTEAM.Presentation.Web.ViewModels.InfoBox
<div class="col-lg-3 col-xs-6">
    <!-- small box -->
    <div class="small-box bg-green">
        <div class="inner">
            <h3>@Model.Dado<sup style="font-size: 20px">%</sup></h3>
            <p>@Model.Descricao</p>
        </div>
        <div class="icon">
            <i class="ion ion-stats-bars"></i>
        </div>
        <a href="@Model.Url" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
    </div>
</div>

